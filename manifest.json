
{
  "manifest_version": 3,
  "name": "Devsloader",
  "version": "1.0",
    "icons": {
    "128": "icons/icon.png"
  },
  "description": "Reloads the page every specified interval with start and stop buttons.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
       "32": "icons/icon.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "js/background.js"
  }
}

